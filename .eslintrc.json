{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "semi": [2, "never"],
    "no-console": "off",
    "no-useless-escape": 0,
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 1
      }
    ]
  },
  "extends": "eslint:recommended",
  "env": {
    "es6": true
  },
  "globals": {
    "browser": true,
    "window": true,
    "console": true,
    "document": true,
    "chrome": true,
    "Seemple": true,
    "$": true
  }
}
